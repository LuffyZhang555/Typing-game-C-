<Window x:Class="WpfApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Starting Window" Height="350" Width="525">
    <Grid Margin="0,0,0,0">
          
        <Grid.Background>
            <ImageBrush ImageSource="/WpfApplication1;component/image/pc.jpg">
            </ImageBrush>
        </Grid.Background>
        <Frame x:Name="main" NavigationUIVisibility="Hidden" Margin="-9,-11,-21,0"/>
        <Button x:Name="start" Background="#FF4A7DDA" Content="START IERG3080" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="120,220,0,0" Width="272" Height="50" Click="ClickButton" FontWeight="Bold" FontSize="13"/>
        <TextBlock x:Name="gamename" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="120,80,0,0" TextWrapping="Wrap" FontSize="45" Width="285" Foreground="DarkBlue" FontWeight="ExtraBlack" Text="Luffy's Typing Game"/>
    </Grid>
</Window>
